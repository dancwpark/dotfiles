# Aliases added by dancwpark for comfort
## alias for preferred ls
alias l='ls -l'
## alias for multilevel cd ..
alias ..='cd ../'
alias ..2='cd ../../'
alias ..3='cd ../../../'
alias ..4='cd ../../../../'
alias ..5='cd ../../../../../'
## alias for much used destinations
alias desktop='cd ~/Desktop'

## For office desktop only! (research stuff)
alias research3='conda activate tf-gpu3'
alias research2='conda activate tf-gpu2'

